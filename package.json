{
  "name": "mission-parser",
  "version": "1.1.0-dev0",
  "description": "Parse IL-2 FB mission file and produce detailed information about mission.",
  "keywords": [
    "il2",
    "il-2",
    "fb",
    "forgotten battles",
    "parser",
    "mission"
  ],
  "license": "LGPL-3.0",
  "author": "Alexander Oblovatniy",
  "homepage": "http://il-2-missions-parser.readthedocs.org/en/latest/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IL2HorusTeam/il2fb-mission-parser.git"
  },
  "bugs": {
    "url": "https://github.com/IL2HorusTeam/il2fb-mission-parser/issues"
  },
  "dependencies": {
    "babel-runtime": "v6.5.0",
    "material-ui": "^0.15.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-dropzone": "^3.4.0",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "babelify": "v7.2.0",
    "babel-preset-es2015": "v6.5.0",
    "babel-preset-react": "v6.5.0",
    "browserify": "v13.0.0",
    "envify": "v3.4.0",
    "reactify": "v1.1.1",
    "uglify-js": "v2.6.1"
  },
  "main": "./src/jsx/app.jsx",
  "scripts": {
    "dev": "browserify . -t [envify --APP_ENV development] | uglifyjs -cm > ./build/bundle.js; sass --update ./src/scss:./build",
    "prod": "browserify . -t [envify --APP_ENV production] | uglifyjs -cm > ./build/bundle.js; sass --update ./src/scss:./build"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./jsx"
  }
}
